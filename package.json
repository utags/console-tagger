{
  "name": "console-tagger",
  "version": "0.0.1",
  "description": "console-tagger",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "p": "prettier --write .",
    "lint": "prettier --write . && xo --fix",
    "test": "jest"
  },
  "keywords": [],
  "author": "Pipecraft",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "xo": "^0.60.0"
  },
  "xo": {
    "space": 2,
    "prettier": true,
    "globals": [
      "document"
    ],
    "envs": [
      "browser"
    ],
    "rules": {
      "import/extensions": 0,
      "unicorn/prefer-module": 0,
      "capitalized-comments": 0
    },
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "rules": {
          "no-undef": 0
        }
      }
    ]
  }
}
