{
  "name": "console-tagger",
  "version": "0.0.5",
  "description": "console-tagger",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "p": "prettier --write .",
    "lint": "prettier --write . && xo --fix",
    "test": "jest"
  },
  "keywords": [
    "console",
    "tagger",
    "tag",
    "console-tagger"
  ],
  "author": "Pipecraft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/utags/console-tagger/issues"
  },
  "homepage": "https://github.com/utags/console-tagger#readme",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "xo": "^0.60.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "xo": {
    "space": 2,
    "prettier": true,
    "globals": [
      "document"
    ],
    "envs": [
      "browser"
    ],
    "rules": {
      "import/extensions": 0,
      "capitalized-comments": 0
    },
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "rules": {
          "no-undef": 0
        }
      }
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "node"
  }
}
